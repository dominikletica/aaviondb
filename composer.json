{
    "name": "dominikletica/aaviondb",
    "description": "aavionDB \u2013 modular PHP-framework for JSON-based storage, versioning & AI-exports",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AavionDB\\": "system/"
        }
    },
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.9",
        "symfony/http-foundation": "^7.3.4",
        "ramsey/uuid": "^4.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "friendsofphp/php-cs-fixer": "^3.88.2",
        "phpstan/phpstan": "^2.1.31"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "cs:fix": "php-cs-fixer fix",
        "analyse": "phpstan analyse"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
